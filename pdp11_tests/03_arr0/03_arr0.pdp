    .=1004                     ; код располагается с 1000 адреса

    clr R0                     ; R0 = 0
    
LOOP:
    mov (R2)+, R3              ; R3 = *R2; R2++ записывает в R3 число, лежащее по адресу, хранимому в R2, этот адрес увеличивается
    beq END
    add R3, R0                 ; очередное число прибавляется к текущей сумме
    br LOOP
END:    
    halt